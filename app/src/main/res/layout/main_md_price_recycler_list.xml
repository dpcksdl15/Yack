<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="166dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/nb"
            android:textColor="#404041"
            android:textSize="14sp"
            android:text="TextView" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:backgroundTint="@color/white"
            app:srcCompat="@drawable/icon_x_18" />
    </LinearLayout>

    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="32dp"
    android:orientation="horizontal"
    android:paddingRight="16dp"
    android:paddingLeft="16dp"
    android:layout_marginTop="16dp"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >
        <Spinner
            android:id="@+id/sp_md_days"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="9.5dp"
            android:paddingLeft="10dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/md_price_search_text"
            android:fontFamily="@font/nr"
            android:textColor="#828282"
            android:text="" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >

        <Spinner
            android:id="@+id/sp_md_day_count"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="9.5dp"
            android:paddingLeft="10dp"
            android:layout_marginLeft="6dp"
            android:background="@drawable/md_price_search_text"
            android:fontFamily="@font/nr"
            android:textColor="#828282" />

    </LinearLayout>


</LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:orientation="horizontal"
        android:paddingRight="16dp"
        android:paddingLeft="16dp"
        android:layout_marginTop="16dp"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            >

        <Spinner
            android:id="@+id/sp_md_count"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingTop="9.5dp"
            android:paddingLeft="10dp"
            android:layout_marginRight="6dp"
            android:background="@drawable/md_price_search_text"
            android:fontFamily="@font/nr"
            android:textColor="#828282"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="6dp"
            android:orientation="horizontal"
            >

        <TextView
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/md_price_toglebox_left"
            android:backgroundTint="#067bfe"
            android:fontFamily="@font/nr"
            android:textColor="#ffffff"
            android:text="보험" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/md_price_toglebox_right"
                android:fontFamily="@font/nr"
                android:textColor="#067bfe"
                android:text="비보험" />

        </LinearLayout>




    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:orientation="horizontal"
        >


        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:fontFamily="@font/nb"
            android:textSize="12sp"
            android:textColor="#828282"
            android:text="적용일자" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:layout_weight="1"
            android:fontFamily="@font/nb"
            android:textSize="12sp"
            android:textColor="#a9a9a9"
            android:text="적용일자" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginRight="6dp"
            app:srcCompat="@drawable/cap_icon" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/neb"
            android:textSize="16sp"
            android:textColor="#404041"
            android:text="100" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/neb"
            android:textSize="16sp"
            android:textColor="#404041"
            android:text="원" />

    </LinearLayout>


    </LinearLayout>